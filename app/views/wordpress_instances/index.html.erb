<h1>Wordpress instances</h1>
<table class="table table-hover" id="wordpress-instances-list">
  <thead>
    <tr>
      <th>Name</th>
      <th>URL</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @wordpress_instances.each do |wp| %>
      <tr>
        <td><%= wp.name %></td>
        <td><%= wp.url %></td>
        <td></td>
      </tr>
    <% end %>
  </tbody>
</table>



