<h1>Update Report for <%= @report.name %></h1>

<div class="nav nav-pills">
  <li class="active"><%= link_to 'Back to Dashboard', wp_installations_path %></li>
</div>

<table class="table table-hover" id="wp-installations-list">
  <thead>
    <tr>
      <th>Name</th>
      <th>URL</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <%= render @report.core_updates %>
    <%= render @report.plugin_updates %>
  </tbody>
</table>
